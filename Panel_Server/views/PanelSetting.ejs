


<%- include('Header.ejs') %>

  <div class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <div class="card">
            <div class="card-header card-header-<%=panelSetting.color_theme%>">
              <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item">
                  <a class="nav-link active" id="setting-tab" data-toggle="tab" href="#setting" role="tab"
                    aria-controls="setting" aria-selected="true"><i class="material-icons">settings</i> Configurações do painel</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="manageserver-tab" data-toggle="tab" href="#manageserver" role="tab"
                    aria-controls="manageserver" aria-selected="false"><i class="material-icons">build</i> Gerenciar servidores</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="listserver-tab" data-toggle="tab" href="#listserver" role="tab"
                    aria-controls="listserver" aria-selected="false"><i class="material-icons">dns</i> Lista de servidores</a>
                </li>

                <li class="nav-item">
                  <a class="nav-link" id="manageserveroffer-tab" data-toggle="tab" href="#manageserveroffer" role="tab"
                    aria-controls="manageserveroffer" aria-selected="false"><i class="material-icons">build</i> Gerenciar ofertas dos servidores</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="listserveroffer-tab" data-toggle="tab" href="#listserveroffer" role="tab"
                    aria-controls="listserveroffer" aria-selected="false"><i class="material-icons">dns</i> Lista de ofertas dos servidores</a>
                </li>

                <li class="nav-item">
                  <a class="nav-link" id="manualoperations-tab" data-toggle="tab" href="#manualoperations" role="tab"
                    aria-controls="manualoperations" aria-selected="false"><i class="material-icons">cached</i> Refresh manual</a>
                </li>
              </ul>
            </div>
            <div class="card-body">
              <div class="tab-content">
                <div class="tab-pane active" id="setting" role="tabpanel" aria-labelledby="setting-tab">
                  <form class="col-md-12" method="post" action="/updatepanelsetting">
                    <div class="col-md-12 row">
                      <!-- Theme Setting -->
                      <div class="col-md-2">
                        <label class="">Tema do painel</label>
                      </div>
                      <div class="col-md-10 row">
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="color_theme" value="danger" id="danger">
                          <label class="form-check-label" for="danger">Vermelho</label>
                        </div>
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="color_theme" value="primary" id="primary">
                          <label class="form-check-label" for="primary">Roxo</label>
                        </div>
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="color_theme" value="warning" id="warning">
                          <label class="form-check-label" for="warning">Laranja</label>
                        </div>
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="color_theme" value="success" id="success">
                          <label class="form-check-label" for="success">Verde</label>
                        </div>
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="color_theme" value="info" id="info">
                          <label class="form-check-label" for="info">Azul</label>
                        </div>
                      </div>
                      <!-- Theme Setting -->
                      <!-- VIP in dash Setting -->
                      <div class="col-md-2">
                        <label class="">Mostra os VIPs no painel público</label>
                        <p><small>Essas configurações só se aplicam para o público.</small></p>
                      </div>
                      <div class="col-md-10 row">
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="dash_vip_show" value="1" id="vipTrue">
                          <label class="form-check-label" for="vipTrue">Sim</label>
                        </div>
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="dash_vip_show" value="0" id="vipFalse">
                          <label class="form-check-label" for="vipFalse">Não</label>
                        </div>
                      </div>
                      <!-- VIP in dash Setting -->
                      <!-- Admin in dash Setting -->
                      <div class="col-md-2">
                        <label class="">Mostrar admins no painel</label>
                        <p><small>Essas configurações se aplicam para o público e para os admins.</small></p>
                      </div>
                      <div class="col-md-10 row">
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="dash_admin_show" value="1" id="adminTrue">
                          <label class="form-check-label" for="adminTrue">Sim</label>
                        </div>
                        <div class="col-md-2">
                          <input class="form-check-input" type="radio" name="dash_admin_show" value="0" id="adminFalse">
                          <label class="form-check-label" for="adminFalse">Não</label>
                        </div>
                      </div>
                      <!-- Admin in dash Setting -->
                      <!-- Normi admin setting -->
                      <%if(adminType===1){%>
                        <div class="col-md-2">
                          <label class="">Normi Admin Basic Settings Access</label>
                        </div>
                        <div class="col-md-10 row">
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="normiadmin_settings" value="1"
                              id="normi_admin_settings1">
                            <label class="form-check-label" for="normi_admin_settings1">Sim</label>
                          </div>
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="normiadmin_settings" value="0"
                              id="normi_admin_settings0">
                            <label class="form-check-label" for="normi_admin_settings0">Não</label>
                          </div>
                        </div>
                        <!-- Normi admin setting -->
                        <!-- Admin login form setting -->
                        <div class="col-md-2">
                          <label class="">Formulário escondido para login de admin</label>
                          <p><small>A rota escondida é /adminlogin</small></p>
                        </div>
                        <div class="col-md-10 row">
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="hiddenadmin_login" value="1"
                              id="hiddenadmin_login1">
                            <label class="form-check-label" for="hiddenadmin_login1">Sim</label>
                          </div>
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="hiddenadmin_login" value="0"
                              id="hiddenadmin_login0">
                            <label class="form-check-label" for="hiddenadmin_login0">Não</label>
                          </div>
                        </div>
                        <!-- Admin login form setting -->
                        <!-- Sales Notif on Discord-->
                        <div class="col-md-2">
                          <label class="">Enviar notificação de venda de VIP no discord</label>
                        </div>
                        <div class="col-md-10 row">
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="salenotification_discord" value="1"
                              id="salenotification_discord1">
                            <label class="form-check-label" for="salenotification_discord1">Sim</label>
                          </div>
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="salenotification_discord" value="0"
                              id="salenotification_discord0">
                            <label class="form-check-label" for="salenotification_discord0">Não</label>
                          </div>
                        </div>
                        <!-- Sales Notif on Discord -->
                        <!-- panel about-->
                        <div class="col-md-2">
                          <label class="">Mostrar card de Autor/Painel na seção Sobre</label>
                        </div>
                        <div class="col-md-10 row">
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="disable_about" value="0"
                              id="disable_about">
                            <label class="form-check-label" for="disable_about">Sim</label>
                          </div>
                          <div class="col-md-2">
                            <input class="form-check-input" type="radio" name="disable_about" value="1"
                              id="disable_about1">
                            <label class="form-check-label" for="disable_about1">Não</label>
                          </div>
                        </div>
                        <!-- panel about -->
                        <%}%>
                          <!-- Community name -->
                          <div class="col-md-2 mt-3">
                            <label class="">Nome da comunidade</label>
                          </div>
                          <div class="col-md-10">
                            <div class="form-group bmd-form-group">
                              <input type="text" name="community_name" class="form-control" id="community_name"
                                required>
                            </div>
                          </div>
                          <!-- Community name  -->
                          <%if(adminType===1){%>
                            <!-- Community logo -->
                            <div class="col-md-2 mt-3">
                              <label class="">URL da logo da comunidade</label>
                            </div>
                            <div class="col-md-10">
                              <div class="form-group bmd-form-group">
                                <input type="url" name="community_logo_url" class="form-control"
                                  id="community_logo_url">
                              </div>
                            </div>
                            <!-- Community logo  -->
                            <!-- Community info -->
                            <div class="col-md-2 mt-3">
                              <label class="">Informações sobre a comunidade</label>
                            </div>
                            <div class="col-md-10">
                              <div class="form-group bmd-form-group">
                                <textarea rows="2" cols="40" maxlength="255" name="community_info" class="form-control"
                                  id="community_info">
                          Enter One line Info or Greating here.....
                        </textarea>
                              </div>
                            </div>
                            <!-- Community info  -->
                            <!-- Community website -->
                            <div class="col-md-2 mt-3">
                              <label class="">Site da comunidade</label>
                            </div>
                            <div class="col-md-10">
                              <div class="form-group bmd-form-group">
                                <input type="url" name="community_website" class="form-control" id="community_website">
                              </div>
                            </div>
                            <!-- Community website  -->
                            <!-- webhook Setting -->
                            <div class="col-md-2 mt-3">
                              <label class="">Discord Webhook URL</label>
                            </div>
                            <div class="col-md-10">
                              <div class="form-group bmd-form-group">
                                <input type="url" name="webhook_url" class="form-control" id="webhook_url">
                              </div>
                            </div>
                            <!-- webhook Setting -->
                            <!-- currency Setting -->
                            <div class="col-md-2 mt-3">
                              <label class="">Moeda da plataforma</label>
                              <p><small>Essa configuração altera a moeda aceita para comprar VIP no site</small></p>
                            </div>
                            <div class="col-md-10">
                              <div class="form-group bmd-form-group">
                                <label class="bmd-label-floating">Moeda</label>
                                <select class="form-control" id="platform_currency" name="platform_currency" required>
                                  <option value="" selected>Selecione o servidor</option>
                                  <option value="USD">USD</option>
                                  <option value="BRL">BRL</option>
                                  <option value="INR">INR</option>
                                </select>
                              </div>
                            </div>
                            <%}%>
                              <!-- currency Setting -->
                    </div>
                    <hr>
                    <button type="submit" onclick="$(this).parents('form').submit()"
                      class="btn btn-success pull-right">Atualizar configurações</button>
                  </form>
                </div>
                <div class="tab-pane " id="manageserver" role="tabpanel" aria-labelledby="manageserver-tab">
                  <%if(adminType===1){%>
                    <div class="col-md-12 row">
                      <form class="col-md-6" id="myForm_addPServer">
