                              <input type="number" name="servertablevipprice" class="form-control"
                                id="servertableVIPPrice_add">
                            </div>
                          </div>
                          <div class="col-md-6">
                            <div class="form-group bmd-form-group">
                              <label class="bmd-label-floating">Moeda</label>
                              <input type="text" name="servertablecurrency" class="form-control"
                                id="servertablecurrency" value="<%=panelSetting.platform_currency%>" disabled>
                            </div>
                          </div>
                        </div>
                        <div class="col-md-12 row">
                          <div class="col-md-6">
                            <div class="form-group bmd-form-group">
                              <label class="bmd-label-floating">Dias de assinatura</label>
                              <input type="number" name="servertabledefaultdays" value="30" class="form-control"
                                id="servertableVIPDays_add">
